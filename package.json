{
  "name": "nodebb-plugin-seo",
  "version": "1.0.0",
  "description": "A plugin for small tweaks to nodebb forums.",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ishwinder/nodebb-plugin-seo"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "seo",
    "shell"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": {
    "name": "ishwinder",
    "email": "singh.ishwinder@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ishwinder/nodebb-plugin-seo/issues"
  },
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^1.10.0 || ^1.11.0 || || ^1.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.3.2",
    "@commitlint/config-angular": "^7.3.1",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.15.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0"
  }
}
